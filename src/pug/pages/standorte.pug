extends ../_template

mixin shops-adress__item(id, title)
  //begin shops-adress__item
  .shops-adress__item(class=id)
    h3.shops-adress__title #{title}
    .shops-adress__contact
      svg
        use(xlink:href="assets/img/sprite.svg#marker")
      a(href="https://goo.gl/maps/CsspFLaubGy" target="_blank" rel="nofollow noopener noreferrer")
        | Erlenstraße 1
        br
        | 12167 Berlin-Steglitz
    .shops-adress__contact
      svg
        use(xlink:href="assets/img/sprite.svg#phone")
      a(href="tel:03061674114") Tel.: 030-61674114
    .shops-adress__contact
      svg
        use(xlink:href="assets/img/sprite.svg#mail")
      a(href="mailto:berlin@4-deluxe.de") berlin@4-deluxe.de

    .shops-adress__schedule
      span Öffnungszeiten:
      span Mo – Fr von 10:00-18:00 Uhr
      span Samstag von 10:00-14:30 Uhr

    span.btn.btn--custom
      span.btn--inner
        a(href="shop-one.html") Mehr erfahren
  //end shops-adress__item



block variables
  - var title = 'Unsere Fachgeschäfte | Delfi Spa'
  - var pageHeaderTitle = 'Unsere Fachgeschäfte'
  - var breadcrumbs01 = 'home'
  - var breadcrumbs03 = 'Unsere Fachgeschäfte'

